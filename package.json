{
	"name": "@playt/client",
	"version": "10.0.4",
	"description": "A client for the PLAYT API",
	"engines": {
		"node": ">= 16.0.0",
		"npm": ">= 8.0.0"
	},
	"author": "PLAYT.net AG",
	"license": "MIT",
	"keywords": ["fetch", "client", "playt"],
	"files": ["src", "dist"],
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs"
		},
		"./browser": {
			"types": "./dist/browser.d.mts",
			"import": "./dist/browser.mjs",
			"default": "./dist/browser.mjs"
		}
	},
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"repository": {
		"type": "git",
		"url": "https://github.com/playt-net/client"
	},
	"bugs": {
		"url": "https://github.com/playt-net/client/issues"
	},
	"homepage": "https://github.com/playt-net/client#readme",
	"scripts": {
		"clean": "rimraf './dist'",
		"build": "npm run clean && npm run typecheck && babel src -d dist --extensions .mts --out-file-extension .mjs",
		"generate": "openapi-typescript https://staging.clashparadise.io/api/docs --output src/types.mts && npm run lint",
		"test": "jest",
		"typecheck": "tsc",
		"lint": "biome check --write",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"@babel/cli": "^7.25.7",
		"@babel/preset-env": "^7.25.7",
		"@babel/preset-typescript": "^7.25.7",
		"@biomejs/biome": "1.9.3",
		"@types/jest": "^29.5.13",
		"@types/node": "^18.19.54",
		"babel-plugin-transform-inline-environment-variables": "^0.4.4",
		"jest": "^29.7.0",
		"node-fetch": "^3.3.2",
		"openapi-typescript": "^6.7.6",
		"rimraf": "^6.0.1",
		"typescript": "~5.6.2"
	},
	"dependencies": {
		"@playt/anybrain-sdk": "^1.2.0",
		"@sentry/browser": "^7.119.0",
		"@sentry/integrations": "^7.119.0",
		"@sentry/node": "^7.119.0",
		"openapi-typescript-fetch": "^2.0.0",
		"slugify": "^1.6.6"
	}
}
