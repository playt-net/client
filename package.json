{
  "name": "@playt/client",
  "version": "4.2.0",
  "description": "A client for the PLAYT API",
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "author": "PLAYT.net AG developers",
  "license": "MIT",
  "keywords": [
    "fetch",
    "client",
    "playt"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.mts",
      "import": "./dist/src/index.mjs"
    },
    "./browser": {
      "types": "./dist/src/browser.d.mts",
      "import": "./dist/src/browser.mjs"
    }
  },
  "module": "./dist/src/index.mjs",
  "types": "./dist/src/index.d.mts",
  "repository": {
    "type": "git",
    "url": "https://github.com/playt-net/client"
  },
  "bugs": {
    "url": "https://github.com/playt-net/client/issues"
  },
  "homepage": "https://github.com/playt-net/client#readme",
  "scripts": {
    "clean": "rimraf './dist'",
    "build": "npm run clean && tsc && npm run build:src && npm run build:external",
    "build:src": "babel src -d dist/src --extensions .mts --out-file-extension .mjs",
    "build:external": "babel --copy-files external -d dist/external --extensions .js --out-file-extension .js",
    "generate": "openapi-typescript https://fun-fair.vercel.app/api/docs --output src/types.ts --prettier-config .prettierrc.json",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/preset-typescript": "7.18.6",
    "@types/node": "18.11.18",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "node-fetch": "3.2.9",
    "openapi-typescript": "5.4.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "vitest": "0.18.1"
  },
  "dependencies": {
    "pusher-js": "7.3.0"
  }
}
