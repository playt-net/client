{
  "name": "@playt/client",
  "version": "3.4.0",
  "description": "A client for the PLAYT API",
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "author": "Leon Machens",
  "license": "MIT",
  "keywords": [
    "fetch",
    "client",
    "playt"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/playt-net/client"
  },
  "bugs": {
    "url": "https://github.com/playt-net/client/issues"
  },
  "homepage": "https://github.com/playt-net/client#readme",
  "scripts": {
    "clean": "rimraf './dist'",
    "build": "npm run clean && tsc --project tsconfig.esm.json",
    "generate": "openapi-typescript https://fun-fair.vercel.app/api/docs --output src/types.ts --prettier-config .prettierrc.json",
    "test": "vitest",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "node-fetch": "3.2.9",
    "openapi-typescript": "5.4.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.7.4",
    "vitest": "0.18.1"
  }
}
